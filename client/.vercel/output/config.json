{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/about\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/dcreply\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/register\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/signout\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/firebase\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/login\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/logout\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/buy\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/course\\/dump\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/course\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/mobile\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/new1\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/rss\\.xml\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/welcome\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}